<% flash.each do |key, value| %>
  <div class="alert alert-<%= flash_bootstrap(key) %> alert-dismissible fade show" role="alert">
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    <ul>
      <li>
        <%= value %>
      </li>
    </ul>
  </div>
<% end %>
